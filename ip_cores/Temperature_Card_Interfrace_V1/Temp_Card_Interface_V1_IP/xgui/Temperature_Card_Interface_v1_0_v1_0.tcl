# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  ipgui::add_page $IPINST -name "Page 0"

  set enable_FPGA_tempports [ipgui::add_param $IPINST -name "enable_FPGA_tempports"]
  set_property tooltip {enables or disables Ports to get the temperaturedata inside the FPGA} ${enable_FPGA_tempports}

}

